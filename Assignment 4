
print ('Assignment 4')
def lsearch(arr,x):
    for i in range(len(arr)):
        if arr[i] == x:
            return i
    return -1

arr=['l','e','t','s','u','p','g','r','a','d','e']
x = 'a'
print("element found at index "+str(lsearch(arr,x)))

x = 'z'
print("element found at index "+str(lsearch(arr,x)))
