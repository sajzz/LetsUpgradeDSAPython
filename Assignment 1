print ('QUESTION NO. 1')
x = int(input("enter the base number: "))
y = int(input("Enter the Exponent: "))
print("Answer to question 1 is " ,x**y)



print ('QUESTION NO. 2')
div_by_3_list = []
h = int(input("Enter upper range limit:"))
for i in range(0,h+1):
   if(i%3==0):
      div_by_3_list.append(i)
      
print(div_by_3_list)      
